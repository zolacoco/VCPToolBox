# .env for FRPSInfoProvider

# Your FRPS server base URL (e.g., http://your-frps-domain.com or http://localhost:7500)
# Do not include the /api part here.
FRPSBaseUrl="http://localhost:7500"

# Your FRPS API admin username
FRPSAdminUser="admin"

# Your FRPS API admin password
FRPSAdminPassword="your_frps_admin_password"

# Set to true to enable verbose debug logging to stderr for this plugin
DebugMode="false"